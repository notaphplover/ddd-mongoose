{
  "name": "cddd-mongoose",
  "version": "0.0.1",
  "description": "CuakLabs Domain Driven Development mongoose integration sample",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:incremental": "tsc --build tsconfig.incremental.json",
    "lint": "eslint src",
    "prebuild": "rimraf dist",
    "test": "mocha",
    "test:js": "mocha --config=\".mocharc.compiled.js\"",
    "test:unit": "mocha --config=\".mocharc.unit.js\""
  },
  "keywords": [
    "CDDD",
    "mongoose"
  ],
  "author": "Roberto Pintos LÃ³pez",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/lodash": "^4.14.166",
    "@types/mocha": "^8.2.0",
    "@types/node": "^12.19.9",
    "@types/sinon": "^9.0.10",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "chai": "^4.2.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "lodash": "^4.17.20",
    "mocha": "^8.2.1",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "inversify": "^5.0.5",
    "mongoose": "^5.11.8"
  }
}
